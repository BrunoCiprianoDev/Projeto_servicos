package teste;

import entidades.*;

public class Teste {
	public static void main(String[] args) {
		Servico servicoUm = new Servico("Limpeza da fachada", 45, 5, 6);
		Servico servicoDois = new Servico("Limpeza da fachada", 45, 5, 6);
		Servico servicoTres = new Servico("Limpeza da fachada", 45, 5, 6);
	
		Cliente clienteUm = new Cliente("José", "da Sila", "dasilva981@ig.com.br", "homem");
		Cliente clienteDois = new Cliente("José", "da Sila", "dasilva981@ig.com.br", "homem");
		Cliente clienteTres = new Cliente("José", "da Sila", "dasilva981@ig.com.br", "homem");
		
		Funcionario funcionarioUm = new Funcionario("Jaquim", "da Silva", "funcionario981@empresa.com.br", "homem");
		Funcionario funcionarioDois = new Funcionario("Jaquim", "da Silva", "funcionario981@empresa.com.br", "homem");
		Funcionario funcionarioTres = new Funcionario("Jaquim", "da Silva", "funcionario981@empresa.com.br", "homem");

		Contrato contratoUm = new Contrato(servicoUm, clienteUm, funcionarioUm);
		Contrato contratoDois = new Contrato(servicoDois, clienteUm, funcionarioDois);
		Contrato contratoTres = new Contrato(servicoTres, clienteTres, funcionarioTres);
		Contrato contratoQuatro = new Contrato(servicoTres, clienteTres, funcionarioTres);
		Contrato contratoCinco = new Contrato(servicoTres, clienteTres, funcionarioTres);
		Contrato contratoSeis = new Contrato(servicoTres, clienteTres, funcionarioTres);
		Contrato contratoSete = new Contrato(servicoTres, clienteTres, funcionarioTres);
		Contrato contratoOito = new Contrato(servicoTres, clienteTres, funcionarioTres);
		Contrato contratoNove = new Contrato(servicoTres, clienteTres, funcionarioTres);
		Contrato contratoDez = new Contrato(servicoTres, clienteTres, funcionarioTres);
	
		Historico historico = new Historico(contratoUm);
		historico.setContrato(contratoDois);
		historico.setContrato(contratoTres);
		historico.setContrato(contratoQuatro);
		historico.setContrato(contratoCinco);
		historico.setContrato(contratoSeis);
		historico.setContrato(contratoSete);
		historico.setContrato(contratoOito);
		historico.setContrato(contratoNove);
		historico.setContrato(contratoDez);
		
		System.out.println(historico.toString());
	
	}

}
